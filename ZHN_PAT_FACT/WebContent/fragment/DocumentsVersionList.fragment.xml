<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">

	<Dialog title="{i18n>TXT_DOCUMENTS_VERSION_LIST_PANEL}" contentWidth="100%"
		class="sapUiSizeCompact">
		<content>
			<Table id="ID_PAT_DASH_DOCUMENTS_VERSION_LIST_TABL"
				noDataText="{i18n>TXT_NO_INFORMATION_AVAILABLE}">
				<columns>
					<Column minScreenWidth="Phone" demandPopin="true" hAlign="Left">
						<Text text="{i18n>TXT_DO_DOC_NAME}" />
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Left">
						<Text text="{i18n>TXT_DOCUMENT_NO}" />
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Left">
						<Text text="{i18n>TXT_CO_DEPARTMENT}" />
					</Column>
					<Column minScreenWidth="Phone" demandPopin="true" hAlign="Left">
						<Text text="{i18n>TXT_DOCUMENTS_STATUS}" />
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Left">
						<Text text="{i18n>TXT_CT_CREATED_ON}" />
					</Column>
					<Column minScreenWidth="Phone" demandPopin="false" hAlign="Center"
						width="12%">
						<Text text="" />
					</Column>
				</columns>
			</Table>

		</content>
		<beginButton>
			<Button text="{i18n>TXT_CLOSE}" app:dialogType="DocumentsVersionList"
				press="onDialogClose" />
		</beginButton>
	</Dialog>

</core:FragmentDefinition>